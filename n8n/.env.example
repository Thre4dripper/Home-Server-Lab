# PostgreSQL Configuration (Optional - for production/heavy usage only)
# For small home lab setups, n8n uses SQLite by default (no PostgreSQL needed)
# Uncomment and configure these if you need better performance or production-grade reliability
# POSTGRES_DB=n8n
# POSTGRES_USER=n8n
# POSTGRES_PASSWORD=n8n_password

# n8n Configuration
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123
N8N_HOST=localhost
N8N_PORT=5678

# Security - Change in production
N8N_ENCRYPTION_KEY=your-encryption-key-change-this

# Timezone
GENERIC_TIMEZONE=UTC