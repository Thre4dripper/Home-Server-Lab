# Rclone Configuration
RCLONE_VERSION=latest
CONTAINER_NAME=rclone
RESTART_POLICY=unless-stopped

# User Configuration
PUID=1000
PGID=1000
TIMEZONE=UTC

# Network Configuration
NETWORK_MODE=bridge

# Port Configuration (for serve commands)
HTTP_PORT=5572
WEBDAV_PORT=5573
FTP_PORT=2121

# Paths
CONFIG_PATH=./config
DATA_PATH=./data

# Rclone Serve Authentication (optional)
RCLONE_USER=admin
RCLONE_PASS=your-password-change-this