# Homarr Dashboard Configuration
HOMARR_TAG=latest
CONTAINER_NAME=homarr
RESTART_POLICY=unless-stopped

# Core Configuration (CHANGE THIS IN PRODUCTION!)
SECRET_ENCRYPTION_KEY=your-64-character-secret-key-change-this-in-production-NOW
NODE_ENV=production
TIMEZONE=UTC

# Network Configuration
HOMARR_PORT=7575
HOMARR_HOST=localhost

# Database Configuration
DB_URL=/appdata/db/db.sqlite
DB_DIALECT=sqlite
DB_DRIVER=better-sqlite3

# Authentication Configuration
AUTH_PROVIDERS=credentials

# Redis Configuration
REDIS_IS_EXTERNAL=false

# Logging Configuration
LOG_LEVEL=info

# Docker Integration
DOCKER_HOSTNAMES=
DOCKER_PORTS=
ENABLE_DOCKER=true

# Resource Limits
MEMORY_LIMIT=1G
MEMORY_RESERVATION=256M