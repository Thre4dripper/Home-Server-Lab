# Twingate Connector Configuration
CONTAINER_NAME=twingate-connector

# Required values from the Twingate Admin console
TWINGATE_NETWORK=your-network-name
TWINGATE_ACCESS_TOKEN=replace-with-access-token
TWINGATE_REFRESH_TOKEN=replace-with-refresh-token

# Optional overrides
TWINGATE_DNS=
TWINGATE_LABEL_HOSTNAME=home-server
TWINGATE_LOG_ANALYTICS=v2
TWINGATE_LABEL_DEPLOYED_BY=docker
